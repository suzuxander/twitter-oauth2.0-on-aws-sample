{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.0.0",
    "generators": {
      "sample": {
        "generatorName": "typescript-axios",
        "output": "gen/sample",
        "inputSpec": "openapi/openapi.yaml"
      },
      "twitter": {
        "generatorName": "typescript-axios",
        "output": "gen/twitter",
        "inputSpec": "openapi/twitter.openapi.yaml"
      }
    }
  }
}
